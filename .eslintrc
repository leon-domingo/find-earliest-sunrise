{
  "root": true,
  "globals": {},
  "rules": {
      "no-undef": 2,
      "no-unused-vars": 2,
      "indent": [2, 2],
      "keyword-spacing": 2,
      "comma-dangle": ["error", {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "always-multiline"
      }],
      "eqeqeq": 1,
      "no-redeclare": 2,
      "object-curly-spacing": [1, "always"],
      "array-bracket-spacing": [1, "never"],
      "func-call-spacing": [2, "never"],
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-else-return": 2,
      "no-empty": 2,
      "no-const-assign": 2,
      "no-trailing-spaces": 2,
      "padded-blocks": [2, "never"],
      "semi": 2
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "parserOptions": {
      "sourceType": "module"
  }
}
